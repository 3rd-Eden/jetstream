{
  "name": "jetstream",
  "version": "0.11.3",
  "description": "Jetstream Sync server framework to sync local and remote models",
  "keywords": [
    "framework",
    "realtime",
    "modular"
  ],
  "author": "Rob Skillington <r@uber.com>",
  "repository": "git://github.com/uber/jetstream.git",
  "main": "index",
  "homepage": "https://github.com/uber/jetstream",
  "contributors": [
    {
      "name": "Rob Skillington"
    },
    {
      "name": "Tuomas Artman"
    }
  ],
  "scripts": {
    "jshint": "jshint --verbose .",
    "test": "tape test/index.js | tap-spec",
    "test-cover": "istanbul cover tape -- test/index.js | tap-spec",
    "view-cover": "istanbul report html && open ./coverage/index.html",
    "test-coveralls": "istanbul cover tape --report lcovonly -- test/index.js | tap-spec && cat ./coverage/lcov.info | coveralls",
    "start": "DEBUG=jetstream* node demos/app.js",
    "check-licence": "uber-licence --dry",
    "add-licence": "uber-licence"
  },
  "pre-commit": [
    "jshint",
    "test",
    "check-licence"
  ],
  "dependencies": {
    "async": "^2.1.2",
    "body-parser": "^1.6.5",
    "callback-or-emit-error": "^1.0.1",
    "debug": "^2.3.2",
    "deep-freeze-strict": "^1.1.0",
    "deepequal": "0.0.1",
    "dotty": "0.0.2",
    "error": "^7.0.2",
    "express": "^4.8.5",
    "lodash": "^4.17.2",
    "maybe-callback": "^2.1.0",
    "node-uuid": "^1.4.1",
    "replr": "^1.0.6",
    "request": "^2.51.0",
    "robb": "^0.2.1",
    "rust-result": "^1.0.0",
    "rwlock": "^5.0.0",
    "semver": "^5.3.0",
    "tryit": "^1.0.1",
    "winston": "^2.3.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.2",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-run": "^0.6.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.5.11",
    "mocha": "^3.1.2",
    "nodemon": "^1.2.1",
    "open": "0.0.5",
    "opn": "^4.0.2",
    "pre-commit": "1.1.3",
    "redtape": "^1.0.0",
    "sinon": "^1.10.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "uber-licence": "^2.0.2",
    "underscore": "^1.6.0"
  },
  "bugs": {
    "url": "https://github.com/uber/jetstream/issues",
    "email": "r@uber.com"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/uber/jetstream/LICENSE"
    }
  ]
}
